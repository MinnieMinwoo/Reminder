generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model userdata {
  id       Int     @id @default(autoincrement())
  nickname String
  email    String  @unique
  password String
  salt     String
  profile  String?
}

model reminddata {
  id       Int     @id @default(autoincrement())
  data     String
  target   String
  time     DateTime
}
